Algoritmo "teste_saida_var2"
//  
//  

Var

  idade : inteiro
  salario : real
   nome : caractere

Inicio

  idade <- 23
  salario <- 2000
  nome <- "Joao Pedro"
  escreval("O funcionario ", nome, " ganha ", salario:8:2, " e tem", idade, "anos.")
  escreval
  escreval("Boa noite!")
  

Fimalgoritmo